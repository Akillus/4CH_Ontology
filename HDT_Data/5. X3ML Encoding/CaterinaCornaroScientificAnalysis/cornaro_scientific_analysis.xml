		<object_analysis id="CY-STARC-XRF-kornaro_leventis_2305">
			<analysis_type>XRF/Spectroscopy</analysis_type>
			<analysis_goal>Pigments identification</analysis_goal>
			<analysis_device>XRF ARTAX-200</analysis_device>
