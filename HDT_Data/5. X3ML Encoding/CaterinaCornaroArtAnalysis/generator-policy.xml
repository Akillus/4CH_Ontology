<?xml version="1.0" encoding="UTF-8"?>
<generator_policy>
   <generator name="SimpleLabel" type="any">
      <pattern>{label}</pattern>
   </generator>
   <generator name="CompositeLabel" type="any">
      <pattern>{term1} {term2}</pattern>
   </generator>
   <generator name="LocalTermURI" prefix="pref" type="any">
      <pattern>{hierarchy}/{term}</pattern>
   </generator>
   <generator name="TypedLiteralGen" type="any">
      <custom generatorClass="gr.forth.TypedLiteralGenerator">
         <set-arg name="text"/>
      </custom>
   </generator>
   <generator name="URIorUUID" type="any">
      <custom generatorClass="gr.forth.URIorUUID">
         <set-arg name="text"/>
      </custom>
   </generator>
   <generator name="ObjectURI" prefix="hdtex" shorten="yes" type="any">
      <pattern>Object/{resource}-{term}</pattern>
   </generator>
   <generator name="ObjectURI-2" prefix="hdtex" shorten="yes" type="any">
      <pattern>Object/{arg1}/{arg2}</pattern>
   </generator>
   <generator name="ObjectLabel" type="any">
      <pattern>Resource {resource} is about object {term}</pattern>
   </generator>
   <generator name="IdentifierURI" prefix="hdtex" type="any">
      <pattern>Identifier/{term}</pattern>
   </generator>
   <generator name="DocumentURI" prefix="hdtex" shorten="yes" type="any">
      <pattern>Document/{term}</pattern>
   </generator>
   <generator name="DocumentURI-2" prefix="hdtex" shorten="yes" type="any">
      <pattern>Document/{provider}/{term}</pattern>
   </generator>
   <generator name="BeliefURI" prefix="hdtex" shorten="yes" type="any">
      <pattern>Belief/{term}</pattern>
   </generator>
   <generator name="BeliefURI-2" prefix="hdtex" shorten="yes" type="any">
      <pattern>Belief/{provider}/{term}</pattern>
   </generator>
   <generator name="EventURI" prefix="hdtex" shorten="yes" type="any">
      <pattern>Event/{resource}-{term}</pattern>
   </generator>
   <generator name="EventURI-2" prefix="hdtex" type="any">
      <pattern>Event/{arg1}/{arg2}</pattern>
   </generator>
   <generator name="EventURI-3" prefix="hdtex" type="any">
      <pattern>Event/{arg1}/{arg2}/{arg3}</pattern>
   </generator>
   <generator name="EventLabel" type="any">
      <pattern>Resource {resource} is about event {term}</pattern>
   </generator>
   <generator name="Time-SpanURI" prefix="hdtex" type="any">
      <pattern>Time-Span/{term}</pattern>
   </generator>
   <generator name="Time-SpanURI-short"
              prefix="hdtex"
              shorten="yes"
              type="any">
      <pattern>Time-Span/{term}</pattern>
   </generator>
   <generator name="PlaceURI" prefix="hdtex" shorten="yes" type="any">
      <pattern>Place/{term}</pattern>
   </generator>
   <generator name="PlaceURI-2" prefix="hdtex" shorten="yes" type="any">
      <pattern>Place/{arg1}/{arg2}</pattern>
   </generator>
   <generator name="AgentURI" prefix="hdtex" type="any">
      <pattern>Agent/{term}</pattern>
   </generator>
   <generator name="AgentURI-short" prefix="hdtex" shorten="yes" type="any">
      <pattern>Agent/{term}</pattern>
   </generator>
   <generator name="PersonURI" prefix="hdtex" type="any">
      <pattern>Person/{term}</pattern>
   </generator>
   <generator name="OrgURI" prefix="hdtex" type="any">
      <pattern>Organisation/{term}</pattern>
   </generator>
   <generator name="ConceptURI" prefix="hdtex" type="any">
      <pattern>Concept/{term}</pattern>
   </generator>
   <generator name="ConceptURI-2" prefix="hdtex" type="any">
      <pattern>Concept/{scheme}/{term}</pattern>
   </generator>
   <generator name="ConceptURI-3" prefix="hdtex" type="any">
      <pattern>Concept/{db}/{scheme}/{term}</pattern>
   </generator>
   <generator name="ConceptURI-2-short"
              prefix="hdtex"
              shorten="yes"
              type="any">
      <pattern>Concept/{scheme}/{term}</pattern>
   </generator>
   <generator name="LangURI" prefix="lexvo" type="any">
      <pattern>{term}</pattern>
   </generator>
   <generator name="OneLevelCustomURI"
              prefix="hdtex"
              shorten="yes"
              type="any">
      <pattern>{level1}/{term}</pattern>
   </generator>
   <generator name="TwoLevelCustomURI"
              prefix="hdtex"
              shorten="yes"
              type="any">
      <pattern>{level1}/{level2}/{term}</pattern>
   </generator>
   <generator name="URNfromTextualContent" type="any">
      <custom generatorClass="gr.forth.TextualContent">
         <set-arg name="text" type="xpath"/>
      </custom>
   </generator>
   <generator name="ConcatMultipleTerms1" type="any">
      <custom generatorClass="gr.forth.ConcatMultipleTerms">
         <set-arg name="prefix" type="constant"/>
         <set-arg name="sameTermsDelim" type="constant"/>
         <set-arg name="diffTermsDelim" type="constant"/>
         <set-arg name="text1"/>
      </custom>
   </generator>
   <generator name="BeliefAdoptionURI-2"
              prefix="hdtex"
              shorten="yes"
              type="any">
      <pattern>BeliefAdoption/{provider}/{term}</pattern>
   </generator>
   <generator name="NarrativeURI-2"
              prefix="hdtex"
              shorten="yes"
              type="instance"
              uuid="no">
      <pattern>Narrative/{provider}/{term}</pattern>
   </generator>
   <generator name="IdentifierURI-2"
              prefix="hdtex"
              shorten="yes"
              type="instance"
              uuid="no">
      <pattern>Identifier/{provider}/{term}</pattern>
   </generator>
</generator_policy>
